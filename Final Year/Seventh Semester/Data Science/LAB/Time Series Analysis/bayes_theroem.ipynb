   "source": [
    "P_A = 0.02 #Probability if disease\n",
    "P_B_given_A = 0.9 #Test positive given disease\n",
    "P_B_given_not_A = 0.05 #Test positive without disease\n",
    "\n",
    "P_not_A = 1 - P_A\n",
    "P_B = P_B_given_A * P_A + P_B_given_not_A * P_not_A\n",
    "P_A_given_B = (P_B_given_A * P_A) / P_B\n",
    "print(f\"P(A|B): {P_A_given_B}\")"
   ]
  }
